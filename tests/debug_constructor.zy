<?php
// 调试构造函数

echo "=== 调试构造函数 ===\n";

// 创建User实例（无参数）
$user1 = new User();
echo "用户1创建完成\n";

// 创建User实例（带命名参数）
$user2 = new User(Name: "Bob");
echo "用户2创建完成\n";

// 检查用户名称
echo "设置用户1名称为Alice\n";
$user1->SetName("Alice");
echo "用户1名称: " + $user1->GetName() + "\n";
echo "用户2名称: " + $user2->GetName() + "\n";

echo "调试完成\n";
?> 