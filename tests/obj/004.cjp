namespace tests\obj;

class Num {
    public function one(){
        return 1;
    }

    public function two(){
        return 2;
    }
}

$obj = new Num();
$a = $obj->one();
$b = $obj->two();

if ($a == 1 && $b == 2) {
    Log::info("类函数调用成功, 多个函数分别返回1和2")
} else {
    Log::fatal("类多个函数分别返回1和2, 异常")
}