namespace tests\basic;

$data = 1 + 2;

if($data == 3) {
    Log::info("加法正确");
}

$data = 3 - 2;

if($data == 1) {
    Log::info("减法正确");
}

$data = 3 * 2;

if($data == 6) {
    Log::info("乘法正确");
}

$data = 6 / 2;

if($data == 3) {
    Log::info("除法正确");
}

$data = 6 % 2;
if($data == 0) {
    Log::info("取余正确01");
}

$data = 6 % 4;
if($data == 2) {
    Log::info("取余正确02");
} else {
    Log::fatal("取余错误");
}