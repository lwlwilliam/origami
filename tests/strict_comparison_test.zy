<?php
// 测试严格相等和严格不等运算符

// 测试严格相等 ===
echo "=== 严格相等测试 ===\n";

// 相同类型和值
$result1 = 5 === 5;
echo "5 === 5: " . ($result1 ? "true" : "false") . "\n";

// 不同类型但值相同
$result2 = 5 === "5";
echo "5 === '5': " . ($result2 ? "true" : "false") . "\n";

// null 比较
$result3 = null === null;
echo "null === null: " . ($result3 ? "true" : "false") . "\n";

$result4 = null === 0;
echo "null === 0: " . ($result4 ? "true" : "false") . "\n";

// 测试严格不等 !==
echo "\n=== 严格不等测试 ===\n";

// 相同类型和值
$result5 = 5 !== 5;
echo "5 !== 5: " . ($result5 ? "true" : "false") . "\n";

// 不同类型但值相同
$result6 = 5 !== "5";
echo "5 !== '5': " . ($result6 ? "true" : "false") . "\n";

// null 比较
$result7 = null !== null;
echo "null !== null: " . ($result7 ? "true" : "false") . "\n";

$result8 = null !== 0;
echo "null !== 0: " . ($result8 ? "true" : "false") . "\n";

// 与普通相等比较
echo "\n=== 与普通相等比较 ===\n";

$result9 = 5 == "5";
echo "5 == '5': " . ($result9 ? "true" : "false") . "\n";

$result10 = 5 === "5";
echo "5 === '5': " . ($result10 ? "true" : "false") . "\n";

$result11 = 5 != "5";
echo "5 != '5': " . ($result11 ? "true" : "false") . "\n";

$result12 = 5 !== "5";
echo "5 !== '5': " . ($result12 ? "true" : "false") . "\n";

// 测试数组和对象的严格比较
echo "\n=== 数组和对象严格比较 ===\n";

$arr1 = [1, 2, 3];
$arr2 = [1, 2, 3];
$arr3 = [1, 2, 4];

$result13 = $arr1 === $arr2;
echo "[1,2,3] === [1,2,3]: " . ($result13 ? "true" : "false") . "\n";

$result14 = $arr1 === $arr3;
echo "[1,2,3] === [1,2,4]: " . ($result14 ? "true" : "false") . "\n";

$result15 = $arr1 !== $arr3;
echo "[1,2,3] !== [1,2,4]: " . ($result15 ? "true" : "false") . "\n";

// 测试浮点数严格比较
echo "\n=== 浮点数严格比较 ===\n";

$result16 = 3.14 === 3.14;
echo "3.14 === 3.14: " . ($result16 ? "true" : "false") . "\n";

$result17 = 3.14 === 3.140;
echo "3.14 === 3.140: " . ($result17 ? "true" : "false") . "\n";

$result18 = 3.14 !== 3.141;
echo "3.14 !== 3.141: " . ($result18 ? "true" : "false") . "\n";

// 测试布尔值严格比较
echo "\n=== 布尔值严格比较 ===\n";

$result19 = true === true;
echo "true === true: " . ($result19 ? "true" : "false") . "\n";

$result20 = false === false;
echo "false === false: " . ($result20 ? "true" : "false") . "\n";

$result21 = true !== false;
echo "true !== false: " . ($result21 ? "true" : "false") . "\n";

// 测试字符串严格比较
echo "\n=== 字符串严格比较 ===\n";

$result22 = "hello" === "hello";
echo "'hello' === 'hello': " . ($result22 ? "true" : "false") . "\n";

$result23 = "hello" === "world";
echo "'hello' === 'world': " . ($result23 ? "true" : "false") . "\n";

$result24 = "hello" !== "world";
echo "'hello' !== 'world': " . ($result24 ? "true" : "false") . "\n";
?>