<?php
// User类反射测试

echo "=== User类反射测试 ===\n";

// 创建User实例（无参数）
$user1 = new User();
$user1->SetName("Alice");

// 创建User实例（带参数）
$user2 = new User(Name: "Bob");

// 获取用户名
string $name1 = $user1->GetName();
echo "用户1名称: " + $name1 + "\n";

string $name2 = $user2->GetName();
echo "用户2名称: " + $name2 + "\n";

// 调用问候方法
string $greeting1 = $user1->Greet();
echo "问候1: " + $greeting1 + "\n";

string $greeting2 = $user2->Greet();
echo "问候2: " + $greeting2 + "\n";

echo "User类反射测试完成\n";
?> 