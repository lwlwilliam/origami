function hello() {
    echo "Hello, World!";
}

function greet(name) {
    echo "Hello, " + name;
}

class TestClass {
    function method1() {
        echo "Method 1";
    }
    
    function method2() {
        echo "Method 2";
    }
}

// 调用函数
hello();
greet("Alice");

// 创建类实例
test = new TestClass();
test.method1();